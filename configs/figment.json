{
    "exp_name": "figment_sq_diff",
    "data":{
        "dataset": "figment",
        "data_dir": "data/figment/",
        "splits": ["Etrain", "Edev", "Etest"],
        "embeddings": true,
        "vocab": true,
        "data_generator": "FigmentDataGenerator"
    },
    "tensorboard_train": false,
    "tensorboard_infer": true,
    "feature_size": 200,
    "label_measurements": 15,
    "type_vocab_size": 102,
    "entities_vocab_size": 201933,
    "embeddings_tune": false,
    "max_to_keep": 5,
    "num_epochs": [10, 9, 5],
    "train": {
        "diff_type": "sq_diff",
        "batch_size": 32,
        "state_size": [784],
        "max_to_keep": 5,
        "hidden_units": 200,
        "lr_feat": 0.001,
        "lr_phi": 0.001,
        "lr_theta": 0.001,
        "lr_psi": 0.00001,
        "lamb_reg_phi": 0.001,
        "lamb_reg_theta": 0.001,
        "lamb_reg_entropy": 1,
        "lamb_pretrain_bias": 1
    },
    "ssvm": {
        "enable": false,
        "steps": 10,
        "eval": false,
        "lr_inference": 0.1
    },
    "eval_print": {
        "f1": false,
        "accuracy": true,
        "energy": false,
        "pretrain": true,
        "infnet": true,
        "f1_score_mode": "examples",
        "threshold": false,
        "time_taken": true
    }
}
